{
  "name": "package.json",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "19"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.3",
    "chart": "^0.1.2",
    "chart.js": "^4.3.0",
    "pinia": "^2.0.35",
    "primeicons": "^6.0.1",
    "primevue": "^3.29.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/bootstrap": "^5.2.6",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.0.4",
    "vite": "^4.3.4",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
